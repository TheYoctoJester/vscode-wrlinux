                    "${workspaceFolder}",
                    "${config:wrlinuxsdk.sdkroot}/sysroots/${targettriplet}/usr/include",
                    "${config:wrlinuxsdk.sdkroot}/sysroots/${hosttriplet}/usr/lib/x86_64-wrs-linux/gcc/x86_64-wrs-linux/10.1.0/include"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "compilerPath": ""
        }
    ],
    "version": 4
